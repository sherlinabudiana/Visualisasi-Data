# Tips and Trick for Python Panda
# 55. menghitung jumlah baris menurut kriteria tertentu


import pandas as pd
import numpy as np


n_rows = 10
n_cols = 5
cols = tuple('ABCDE')

df = pd.DataFrame(np.random.randint(1,10, size = (n_rows, n_cols)), column = cols)

(df['A'] < 5).sum()
(df['A'] < 5).mean()


